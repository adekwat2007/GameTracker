<UserControl x:Class="GameTracker.Views.LibraryPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GameTracker.Views"
             xmlns:vm="clr-namespace:GameTracker.ViewModels"
             xmlns:fa="clr-namespace:FontAwesome5;assembly=FontAwesome5.Net"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance vm:LibraryViewModel, IsDesignTimeCreatable=True}">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/LibraryDataGrid.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <DockPanel>
        <Border Background="#1d2235" CornerRadius="20">

        </Border>
    </DockPanel>
</UserControl>